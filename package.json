{
  "name": "@transferwise/account-details-frontend",
  "version": "0.3.0",
  "description": "TransferWise components for interacting with Account Details",
  "license": "MIT",
  "scripts": {
    "pretest": "",
    "test": "npm run build; karma start karma-conf.js --single-run",
    "test:dev": "karma start karma-conf.dev.js --single-run",
    "prestart": "npm install",
    "start": "npm run less && webpack --watch;",
    "dev": "npm run copy:demo && live-server --port=1666 --open=demo/index.html",
    "build": "npm run less && webpack -p && npm run copy:demo",
    "htmllint": "htmllint src/**/*.html && htmllint src/**/**/*.html && htmllint demo/**/*.html",
    "jshint": "jshint src/**/*.js --exclude-path .jshintignore",
    "eslint": "eslint src",
    "lint": "npm run htmllint && npm run jshint && npm run eslint",
    "less": "lessc demo/**/*.less demo/css/examples.css",
    "copy:jquery": "cp node_modules/jquery/dist/jquery.min.js demo/lib/",
    "copy:ui-router": "cp node_modules/@uirouter/angularjs/release/angular-ui-router.js demo/lib/",
    "copy:styleguide": "cp node_modules/@transferwise/styleguide-components/js/styleguide-components.js demo/lib/",
    "copy:angular": "cp node_modules/angular/angular.min.js demo/lib/",
    "copy:bootstrap": "cp node_modules/bootstrap/dist/js/bootstrap.min.js demo/lib/; cp node_modules/bootstrap/dist/css/bootstrap.css demo/lib/",
    "copy:fonts": "mkdir demo/fonts; cp node_modules/bootstrap/fonts/*.* demo/fonts",
    "copy:demo": "mkdir demo/lib; npm run copy:jquery; npm run copy:angular; npm run copy:bootstrap; npm run copy:fonts; npm run copy:ui-router; npm run copy:styleguide",
    "webpack": "webpack --watch",
    "copy-files-to-dist": "mkdir -p dist && cp package.json CHANGELOG.md README.md dist/",
    "deploy": "git-directory-deploy --directory dist --branch releases --username CircleCI --email circleci@transferwise.com",
    "deploy:demo": "gh-pages -d demo",
    "release-to-github": "release-to-github-with-changelog --branch=releases"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/account-details-frontend",
    "url": "https://github.com/transferwise/account-details-frontend.git"
  },
  "keywords": [
    "AngularJS",
    "Bootstrap"
  ],
  "bugs": {
    "url": "https://github.com/transferwise/account-details-frontend/issues"
  },
  "homepage": "https://github.com/transferwise/account-details-frontend",
  "dependencies": {
    "angular": "^1.7.4",
    "jquery": "^3.3.1",
    "@transferwise/styleguide-components": "^3.4.1"
  },
  "devDependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular-mocks": "^1.7.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "git+https://github.com/transferwise/bootstrap.git",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "gh-pages": "^1.2.0",
    "git-directory-deploy": "^1.5.1",
    "html-loader": "^0.5.5",
    "htmllint": "^0.6.0",
    "htmllint-cli": "0.0.6",
    "jasmine-core": "^2.99.1",
    "jshint": "^2.9.6",
    "jshint-loader": "^0.8.4",
    "json-loader": "^0.5.7",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "less": "^2.7.3",
    "live-server": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.16",
    "release-to-github-with-changelog": "^1.2.0",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.11.3"
  }
}
